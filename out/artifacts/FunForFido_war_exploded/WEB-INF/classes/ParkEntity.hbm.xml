<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.jason.funForFido.entity.ParkEntity" table="park">
        <meta attribute="class-description">
            This class contains the parkEntity detail.
        </meta>

        <id name="parkId" type="int" column="ParkID"><generator class="native"/></id>
        <property name="parkName" column="parkName" type="string" />
        <property name="longitude" column="longitude" type="string"/>
        <property name="latitude" column="latitude" type="string"/>
        <property name="address" column="address" type="string"/>
        <property name="city" column="city" type="string"/>
        <property name="state" column="state" type="string" />
        <property name="zip" column="zip" type="string"/>
    </class>
</hibernate-mapping>